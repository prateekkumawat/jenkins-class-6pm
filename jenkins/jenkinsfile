pipeline {
    agent any 
    
    stages {
        stage("AppCode Checkout"){
            steps{
                git branch:"main", url:"https://github.com/prateekkumawat/jenkins-class-6pm.git"
            }
        }

        stage("run script"){
            steps{
                chmod +x script.sh
                sh ./script.sh 
            }
        }
        
        stage("post Actions"){
            steps{
                echo "performs post actions"
            }
        }
    }
}